{
    "openapi": "3.0.0",
    "info": {
        "title": "RentalX Documentation",
        "description": "This is a Car Rental Service API",
        "version": "1.0.0",
        "contact": {
            "email": "nataliabf@outlook.com"
        }
    },
    "paths": {
        "/categories": {
            "post": {
                "tags": ["Categories"],
                "summary": "Create a category",
                "description": "Create a new category",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/definitions/Category"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Created"
                    },
                    "400": {
                        "description": "Error"
                    }
                }
            },
            "get": {
                "tags": ["Categories"],
                "summary": "List all categories",
                "description": "List all categories",
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/definitions/Category"
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Error"
                    }
                }
            }
        },
        "/categories/import": {
            "post": {
                "tags": ["Categories"],
                "summary": "Import categories from file.",
                "description": "Reads list of categories from a csv file and saves then in the database.",
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "file": {
                                        "type": "string",
                                        "format": "binary"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Created"
                    },
                    "400": {
                        "description": "Error"
                    }
                }
            }
        },
        "/specifications": {
            "post": {
                "tags": ["Specifications"],
                "summary": "Create a specification",
                "description": "Create a new specification",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/definitions/Specification"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Created"
                    },
                    "400": {
                        "description": "Error"
                    }
                }
            },
            "get": {
                "tags": ["Specifications"],
                "summary": "List all specifications",
                "description": "List all specifications",
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "name": {
                                                "type": "string"
                                            },
                                            "description": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Error"
                    }
                }
            }
        },
        "/users": {
            "post": {
                "tags": ["Users"],
                "summary": "Create a user",
                "description": "Create a new user",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/definitions/User"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Created"
                    },
                    "400": {
                        "description": "Error"
                    }
                }
            }
        },
        "/sessions": {
            "post": {
                "tags": ["Users"],
                "summary": "Authenticate a user",
                "description": "Authenticate a user",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "format": "email"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "Password Sample"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Success"
                    },
                    "400": {
                        "description": "Error"
                    }
                }
            }
        }
    },
    "definitions": {
        "Specification": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string",
                    "example": "Specification Name Sample"
                },
                "description": {
                    "type": "string",
                    "example": "Specification Description Sample"
                }
            }
        },
        "Category": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string",
                    "example": "Category Name Sample"
                },
                "description": {
                    "type": "string",
                    "example": "Category Description Sample"
                }
            }
        },
        "User": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string",
                    "example": "Name Sample"
                },
                "email": {
                    "type": "string",
                    "format": "email"
                },
                "password": {
                    "type": "string",
                    "example": "Password Sample"
                },
                "driver_license": {
                    "type": "string",
                    "example": "85057398900"
                }
            }
        }
    }
}
